@model IEnumerable<Collections.Domain.Entities.ApplicationUser>

<p><a asp-action="Index" asp-controller="Admin" class="btn btn-sm btn-primary">Users</a></p>
<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Email</td>
            <td>
                <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Access rights</a>
            </td>
        </tr>
    }
</table>